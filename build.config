# Comment out a line to disable

# If DEVELOPMENT_BUILD is true, then aesalon will output a lot of debugging
# information
set(DEVELOPMENT_BUILD true)

# Build the monitor?
set(BUILD_MONITOR true)
# Build the GUI?
set(BUILD_GUI true)

# Only applicable if the monitor is being built
# AESALON_PLATFORM can be:
#   0: linux x86
#   1: linux x86-64 (sometimes known as amd64)
set(AESALON_PLATFORM        1)

